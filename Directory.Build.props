<Project>
	<PropertyGroup>
		<Version>2.2.5.0</Version>
		<AssemblyVersion>2.2.5.0</AssemblyVersion>
		<ProductName>AForge.NET</ProductName>
		<Product>AForge.NET (modified by exocad GmbH, originally based work on of AForge and Andrew Kirillov)</Product>
		<Copyright>Copyright Â© exocad GmbH 2021-2024, AForge 2012</Copyright>
		<Authors>exocad GmbH, AForge and Andrew Kirillov</Authors>
		<Company>exocad GmbH based on work of AForge</Company>

		<PackageProjectUrl>https://github.com/exocad/AForge.NET</PackageProjectUrl>
		<RepositoryUrl>https://github.com/exocad/AForge.NET</RepositoryUrl>

		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<IsPackable>true</IsPackable>
		<PackageLicenseFile>License.txt</PackageLicenseFile>
		<PackageReadMeFile>README.md</PackageReadMeFile>
	</PropertyGroup>

	<PropertyGroup>
		<TargetFrameworks>net48;net6.0-windows;net8.0-windows</TargetFrameworks>
		<UseArtifactsOutput>true</UseArtifactsOutput>
	</PropertyGroup>
</Project>
